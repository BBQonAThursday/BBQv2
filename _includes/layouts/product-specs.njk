<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/main.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Cabin:wght@400;700&family=Roboto+Condensed:wght@400;700&display=swap" rel="stylesheet"> 
  </head>
  <body>
   
   {% include "components/header.njk" %}
    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
    <div class="product-specs-container relative w-full bg-black h-auto max-w-7xl mx-auto border-black border-8">
      <img class="product-image" alt="graber-4143-layered-shades-rs20-v1.tif" src="https://embed.widencdn.net/img/springswindowfashions/1402zt82bz/1280x800px/graber-4143-layered-shades-rs20-v1.jpg?crop=true&keep=c&q=80&color=ffffffff&u=fpq9bh">
      <div class="product-spec absolute inset-1/3 bg-black border-8 w-5 h-5" id="spec-1"></div>
      <div class="product-spec absolute inset-1/2 bg-black border-8 w-5 h-5" id="spec-2"></div>
      <div class="product-spec absolute inset-1/4 bg-black border-8 w-5 h-5" id="spec-3"></div>
    </div>
      {{ content | safe }}
    </main>
     
  </body>
</html>
